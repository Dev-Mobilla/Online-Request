    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Online Request</title>
    <link rel="icon" href="~/Content/images/Diamante.png">
    @Styles.Render("~/Content/css")
    <meta name="description" content="Mlhuillier - Online Request System is a web-based system where MMD requests are automated via web portal." />
    <script type="text/javascript">
        var Url = '@Request.ApplicationPath';
        if (Url == '/OnlineRequestSystem') {
            Url = '/OnlineRequestSystem/';
        } else {
            Url = "";
        }
    </script>
</head>

<body>
    <div class="navbar navbar-default navbar-fixed-top headBoxshadow">
        <div style="padding:0 50px 0 50px;">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a href="@Url.Action("ViewOpenRequest", "OpenRequest")">
                    <img src="~/Content/images/Diamante.png" style="height:40px;margin-top:5px;padding-right:20px;" />
                </a>
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li>
                        <a id="create-req" style="color:black;" onclick="loadClick('Normal')" href="@Url.Action("RequestFormView", "Request")">Create Request</a>
                    </li>
                    @if (Session["OpenRequest"] == "Div")
                    {
                        { Html.RenderPartial("~/Views/Shared/NavMenus/_divmenu.cshtml"); }
                    }
                    else if (Session["OpenRequest"] == "GMO")
                    {
                        { Html.RenderPartial("~/Views/Shared/NavMenus/_gmomenu.cshtml"); }
                    }
                    else
                    {
                        <li>
                            <a style="color:black;" onclick="loadClick('Open')" href="@Url.Action("ViewOpenRequest", "OpenRequest")">Open Requests</a>
                        </li>
                    }
                    <li>
                        <a style="color:black;" onclick="loadClick('Pending')" href="@Url.Action("PendingRequests", "Pending")">Pending Requests</a>
                    </li>
                    @if (Convert.ToInt32(Session["isVPAssistant"]) == 1)
                    {
                        <li>
                            <a style="color:black;" onclick="loadClick('Branch')" href="@Url.Action("BranchRequests", "VPAssistant")">Branch Requests</a>
                        </li>
                    }
                    <li class="menu-item dropdown">
                        <a href="#" style="color:black;" class="dropdown-toggle" data-toggle="dropdown">
                            Closed Requests&nbsp;<b class="caret"></b>
                        </a>
                        <ul class="dropdown-menu">
                            <li>
                                <a onclick="loadClick('Close')" href="@Url.Action("ViewCloseRequest", "CloseRequest", new { c = "CLOSED" })">Closed Requests</a>
                            </li>
                            <li>
                                <a onclick="loadClick('Disapprove')" href="@Url.Action("ViewCloseRequest", "CloseRequest", new { c = "DISAPPROVED" })">Disapproved Requests</a>
                            </li>
                        </ul>
                    </li>

                    <li class="menu-item dropdown">
                        <a href="#" style="color:black;" class="dropdown-toggle" data-toggle="dropdown">
                            Reports&nbsp;<b class="caret"></b>
                        </a>
                        <ul class="dropdown-menu">
                            <li>
                                <a onclick="loadClick('Normal')" href="@Url.Action("ReportsMenu", "Reports", new { type = "daily" })"><i class="fa fa-list-alt"></i> &nbsp;Daily Report</a>
                            </li>
                            <li>
                                <a onclick="loadClick('Normal')" href="@Url.Action("ReportsMenu", "Reports", new { type = "monthly" })"><i class="fa fa-list-alt"></i> &nbsp;Monthly Report</a>
                            </li>
                            @if (Convert.ToInt32(Session["Allowed"]) == 1)
                            {
                                <li>
                                    <a target="_blank" href="@Session["ReportUrl"]"><i class="fa fa-bar-chart"></i> &nbsp;Graphical Report</a>
                                </li>
                            }
                        </ul>
                    </li>
                    @if ((Session["UserSession"] as OnlineRequestSystem.Models.ORSession).s_job_title == "TELLER" || (Session["UserSession"] as OnlineRequestSystem.Models.ORSession).s_job_title == "BM" || (Session["UserSession"] as OnlineRequestSystem.Models.ORSession).s_job_title == "ABM" || (Session["UserSession"] as OnlineRequestSystem.Models.ORSession).s_job_title == "AREA MANAGER" || (Session["UserSession"] as OnlineRequestSystem.Models.ORSession).s_job_title == "RAM" || (Session["UserSession"] as OnlineRequestSystem.Models.ORSession).s_task == "GMO-ASTGENMAN" || (Session["UserSession"] as OnlineRequestSystem.Models.ORSession).s_task == "GMO-GENMAN")
                    {
                        <li>
                            <a style="color:black;" onclick="loadClick('Normal')" href="@Url.Action("CashRequest", "Request")">Cash Request</a>
                        </li>
                    }
                </ul>

                <ul class="nav navbar-nav"></ul>
                @Html.Partial("_LoginPartial")
            </div>
        </div>
    </div>
    <div class="container body-content">
        @RenderBody()
        <footer data-role="footer" data-position="fixed" class="navbar-fixed-bottom" style="z-index: -1;">
            <div style="font-size:10px;height:15px;">
                <center><p>M.LHUILLIER FINANCIAL SERVICES, INC.</p></center>
            </div>
        </footer>
    </div>

    @{ Html.RenderPartial("~/Views/Shared/_Loader.cshtml");}

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/jqueryval")
    @Scripts.Render("~/bundles/jquery-ui")
    @Scripts.Render("~/bundles/modernizr")
    <script src="~/Scripts/SourceNotAllowed.js"></script>
    @RenderSection("scripts", required: false)
    @RenderSection("tableCss", required: false)
    <script src="~/Scripts/Loaders/loaders.js"></script>

 
</body>
</html>