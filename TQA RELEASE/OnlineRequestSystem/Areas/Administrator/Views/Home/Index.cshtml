@{
    Layout = null;
}
<br />


<!DOCTYPE html>
<html>
<head>

    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Online Request</title>
    <link rel="icon" href="~/Content/images/Diamante.png">
    @Styles.Render("~/Content/css")
    <meta name="description" content="Mlhuillier - Online Request System is a web-based system where MMD requests are automated via web portal." />
    <script type="text/javascript">
        var Url = '@Request.ApplicationPath';
        if (Url == '/OnlineRequestSystem') {
            Url = '/OnlineRequestSystem/';
        } else {
            Url = "";
        }
    </script>
</head>

<body>

    <div class="container body-content">
        <div class="col-md-12">
            <div class="col-md-3"></div>
            <form action="/Administrator/Home/Authenticate" method="post" class="jumbotron col-md-6">
                <center>
                    <div class="row">
                        <h4>Security Login, Please enter password.</h4>
                        <br />
                        <input class="form-control" name="key" id="key" type="password" />
                        <br />
                        <button class="btn btn-danger" type="submit">Submit</button>
                    </div>
                    @if (ViewBag.Msg != null)
                    {
                        <span>@ViewBag.Msg</span>
                    }
                </center>
            </form>
            <div class="col-md-3"></div>
        </div>
        <footer data-role="footer" data-position="fixed" class="navbar-fixed-bottom" style="z-index: -1;">
            <div style="font-size:10px;height:15px;">
                <center><p>M.LHUILLIER FINANCIAL SERVICES, INC.</p></center>
            </div>
        </footer>
    </div>

    <script src="~/Scripts/SourceNotAllowed.js"></script>

</body>
</html>