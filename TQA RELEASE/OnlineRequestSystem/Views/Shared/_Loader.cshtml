<div class="modal fade modal-inactivity" data-backdrop="static" data-keyboard="false" style="z-index:1041;">
    <div class="modal-dialog modal-custom" style="width:35%;min-width:35%;">
        <div class="modal-content">
            <div class="modal-header border-disabler">
                <h4 class="modal-title"></h4>
            </div>
            <div class="modal-body">
                <p class="l-fontsize14">Your session has expired due to an extended period of inactivity.</p>
            </div>
            <div class="modal-footer">
                <div class="form-control-static" style="float:right;padding:0;margin:0;margin-top:-12px;margin-bottom:-13px;">
                    <input type="button" value="Back to login" class="btn btn-sm btn-success btn-danger" onclick="location.href='@Url.Action("Logout","Userlogin")'" />
                </div>
            </div>
        </div>
    </div>
</div>

<center>
    <div class="modal fade my-modal loginloader" data-backdrop="static" data-keyboard="false" style="display:none;overflow:no-display ;">
        <div class="modal-dialog modal-sm" style="width:130px;">
            <div class="modal-content" style="overflow:hidden;">
                <div class="modal-body pull-left msg" style="border-radius:2px;">
                    &nbsp;&nbsp;<span class="glyphicon glyphicon-refresh glyphicon-refresh-animate "></span>
                    &nbsp;Loading..
                </div>
            </div>
        </div>
    </div>
</center>

<div id="loginmodal" class="modal fade modal-popup" data-backdrop="static" data-keyboard="false" style="display:none;overflow:no-display">
    <div class="modal-dialog" role="document">
        <div class="modal-content col-md-7" style="margin-left:22.5%;">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h5 class="modal-title" id="myModalLabel"><b>SYSTEM MESSAGE</b></h5>
            </div>
            <div class="modal-body borderless" id="loginmodalword">
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-danger" data-dismiss="modal">
                    <span class="glyphicon glyphicon-ok"></span> Ok
                </button>
            </div>
        </div>
    </div>
</div>

